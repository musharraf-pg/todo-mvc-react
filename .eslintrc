{
    "extends": "@prodigy/eslint-config-prodigy",
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/jsx-uses-vars": 2,
        "react/jsx-uses-react": 2,
        "react/react-in-jsx-scope": 2
    },
    "plugins": ["react"]
}
